# Configurações de Produção - Gabi Multitenancy
# Copie este arquivo para .env e configure as variáveis

# Domínio principal
DOMAIN=gabi.yourdomain.com

# Banco de dados
DB_PASSWORD=gabi_prod_2024_secure_password
REDIS_PASSWORD=gabi_redis_prod_2024_secure_password

# Chaves de segurança
OS_SECURITY_KEY=gabi_prod_security_key_2024_very_secure

# APIs de IA
OPENAI_API_KEY=sk-your-openai-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Configurações do modelo
DEFAULT_MODEL=gpt-4o

# SSL/TLS
ACME_EMAIL=admin@yourdomain.com

# Configurações de produção
NODE_ENV=production
AGNO_LOG_LEVEL=INFO
AGNO_ENABLE_TELEMETRY=false

# Configurações de recursos
DB_MEMORY_LIMIT=2G
DB_CPU_LIMIT=1.0
REDIS_MEMORY_LIMIT=512M
REDIS_CPU_LIMIT=0.5
GABI_OS_MEMORY_LIMIT=2G
GABI_OS_CPU_LIMIT=2.0
GABI_INGEST_MEMORY_LIMIT=1G
GABI_INGEST_CPU_LIMIT=1.0
GABI_CHAT_MEMORY_LIMIT=1G
GABI_CHAT_CPU_LIMIT=1.0
