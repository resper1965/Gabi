# ── PostgreSQL ──
GABI_DATABASE_URL=postgresql+asyncpg://gabi:gabi@localhost:5432/gabi

# ── Google Cloud ──
GABI_GCP_PROJECT_ID=your-gcp-project
GABI_VERTEX_AI_LOCATION=us-central1

# ── AI Models ──
GABI_MODEL_GHOST=gemini-2.0-flash-001
GABI_MODEL_LAW=gemini-1.5-pro
GABI_MODEL_NTALK=gemini-2.0-flash-001

# ── Embeddings ──
GABI_EMBEDDING_MODEL_NAME=BAAI/bge-m3

# ── Firebase Admin ──
GABI_FIREBASE_PROJECT_ID=your-firebase-project
GABI_FIREBASE_ADMIN_SERVICE_ACCOUNT=path/to/service-account.json

# ── CORS ──
GABI_CORS_ORIGINS=["http://localhost:3000","https://gabi.ness.com.br"]

# ── nTalkSQL ──
GABI_MAX_QUERY_ROWS=1000
GABI_QUERY_TIMEOUT_SECONDS=30
