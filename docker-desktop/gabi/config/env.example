# Gabi - Chat Multi-Agentes
# Configurações para Docker Desktop Local

# Domínio
DOMAIN=localhost

# Banco de dados
POSTGRES_USER=gabi
POSTGRES_PASSWORD=gabi_dev_secure_2024
POSTGRES_DB=gabi_dev

# Redis
REDIS_PASSWORD=gabi_redis_2024

# Chaves de segurança
OS_SECURITY_KEY=gabi_dev_security_key_2024

# APIs de IA (configure suas chaves)
OPENAI_API_KEY=sk-your-openai-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Configurações do modelo
DEFAULT_MODEL=gpt-4o

# Configurações de desenvolvimento
NODE_ENV=development
AGNO_LOG_LEVEL=DEBUG
AGNO_ENABLE_TELEMETRY=false

# Portas
DB_PORT=5432
REDIS_PORT=6379
GABI_OS_PORT=7777
GABI_CHAT_PORT=3000
GABI_INGEST_PORT=8000
TRAEFIK_PORT=80
TRAEFIK_HTTPS_PORT=443
TRAEFIK_DASHBOARD_PORT=8080
